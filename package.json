{
  "name": "@mark/frontend",
  "version": "0.0.0",
  "engines": {
    "node": ">= 18",
    "pnpm": ">= 9"
  },
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "add": "turbo gen workspace --type packages",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8",
    "turbo": "latest"
  },
  "packageManager": "pnpm@9.12.3"
}
